<Window x:Class="WPF_PT_LAB2_ATTEMPT3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_PT_LAB2_ATTEMPT3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ToolBar HorizontalAlignment="Left" Height="30" Margin="10,10,0,0" VerticalAlignment="Top" Width="50" Header="File">
            <MenuItem x:Name="Open" Header="Open" Click="Open_Click"/>
            <MenuItem x:Name="Exit" Header="Exit" Click="Exit_Click"/>
        </ToolBar>
        <TreeView x:Name="treeView" Margin="0,54,500,0">
        </TreeView>

    </Grid>
</Window>
